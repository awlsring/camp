{
    "version": "2.0",
    "sources": ["model"],
    "imports": ["../common/model"],
    "maven": {
      "dependencies": [
          "software.amazon.smithy:smithy-model:1.37.0",
          "software.amazon.smithy:smithy-validation-model:1.37.0",
          "software.amazon.smithy:smithy-aws-traits:1.37.0",
          "software.amazon.smithy:smithy-linters:1.37.0",
          "software.amazon.smithy:smithy-openapi:1.37.0",
          "software.amazon.smithy:smithy-openapi-traits:1.37.0",
          "software.amazon.smithy:smithy-waiters:1.37.0",
          "software.amazon.smithy.rust.codegen:codegen-core:0.1.0",
          "software.amazon.smithy.rust.codegen:codegen-client:0.1.0"
        ]
    },
    "projections": {
      "openapi": {
        "plugins": {
          "openapi": {
            "service": "awlsring.camp.local#CampLocal",
            "protocol": "aws.protocols#restJson1"
          }
        }
      },
      "rust": {
        "plugins": {
            "rust-client-codegen": {
                "runtimeConfig": {
                    "relativePath": "/home/awlsring/Code/smithy-rs/rust-runtime"
                },
                "service": "awlsring.camp.local#CampLocal",  
                "codegen": {
                    "includeFluentClient": true
                },
                "module": "camp_local_rs",
                "moduleDescription": "Camp Local Client",
                "moduleVersion": "0.1.0",
                "moduleAuthors": ["awlsring@gmail.com"]
            }
        }
      }
    }
  }